.nav-mobile {
	.site-nav__link{
		&:hover{
			background:transparent;
    		&:after{
    			display: none;
    		}
    	}
	}
}
.site-nav {
	font-size: em(14px);
	cursor: default;
	margin: 0;
	display: inline-block;
	text-align: left;
	li {
		margin: 0;
		display: block;
	}

	> li {
		position: relative;
		display: inline-block;
		padding:0;
		float: left;
		&.site-nav--active{
			> a{
				&:before{
					width: 100%;
					opacity: 1;
				}
			}
		}
		a{
			display: inline-block;
			padding: 15px 20px;
			line-height: 20px;
			&:before{
			    background-color: $colorDefault;
			    content: "";
			    height: 100%;
			    left: 50%;
			    opacity: 0;
			    position: absolute;
			    top: 50%;
			    transform: translate(-50%, -50%);
			    transition: all 0.3s ease 0s;
			    width: 0;
			    z-index: -1;
			}
		}
		&:hover,&:focus,&:active{
			> a{
				background:$colorDefault;
				color:#fff;
				&:before{
					opacity: 1;
    				width: 100%;
				}
			}
		}
	}
}


