/*============================================================================
	#Site Header
==============================================================================*/
.site-header {
	position:relative;
	&:after{
    		background: {{settings.header_background_color}};
		    content: "";
		    height: 100%;
		    left: 0;
		    margin-left: -50%;
		    position: absolute;
		    top: 0;
		    width: 200%;
		    z-index: -4;
    	}
	@include at-query($max, $large){
		.site-header__logo{display: block; padding: 10px 0;}
	}
	@include at-query($postSmall, $medium) {
		.site-header__logo{display: block; padding: 0;}
	}
	.grid--table {
		display: table;
		table-layout: fixed;
		width: 100%;

		> .grid__item {
			float: none;
			display: table-cell;
			vertical-align: middle;
		}
	}
}
.topbar-head{
    	padding: 6px 0;
    	position: relative;
    	&:after{
    		background: {{ settings.topbar_background_color}};
		    content: "";
